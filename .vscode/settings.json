{
    "editor.rulers": [
        88
    ],
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/__pycache__": true,
        "**/.ipynb_checkpoints": true,
        "**/.pytest_cache": true,
        "**/.ropeproject": true
    },
    "python.pythonPath": "/anaconda/envs/rst-lsp/bin/python",
    "python.linting.pylintEnabled": false,
    "python.linting.flake8Enabled": true,
    "python.linting.enabled": true,
    "python.formatting.provider": "black",
    "python.testing.pytestArgs": [
        "tests"
    ],
    "python.testing.unittestEnabled": false,
    "python.testing.nosetestsEnabled": false,
    "python.testing.pytestEnabled": true,
    "yaml.schemas": {
        "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json": "**/restructuredtext.tmLanguage.yaml"
    },
    "cSpell.words": [
        "docutils"
    ]
}